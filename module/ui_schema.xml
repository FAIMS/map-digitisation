<?xml version='1.0' encoding='utf-8'?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>FILL THIS IN</h:title>
    <model>
      <instance>
        <faims id="FILL_THIS_IN">
          <!--START: Style-->
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <!--END: Style-->
          <User>
            <User>
              <User_List/>
            </User>
          </User>
          <Control>
            <Simple_Forms>
              <New_Simple_Form/>
              <Existing_Simple_Forms/>
            </Simple_Forms>
            <Map>
              <Map/>
              <Center_Me_1/>
            </Map>
          </Control>
          <Simple_Form>
            <Simple_Form>
              <ID/>
              <Name/>
              <Colgroup_1>
                <Col_1>
                  <Latitude/>
                </Col_1>
                <Col_2>
                  <Longitude/>
                </Col_2>
              </Colgroup_1>
              <Colgroup_2>
                <Col_1>
                  <Northing/>
                </Col_1>
                <Col_2>
                  <Easting/>
                </Col_2>
                <Col_3>
                  <Accuracy/>
                </Col_3>
              </Colgroup_2>
              <Take_From_GPS_1/>
              <Dropdown/>
              <Simple_Form_timestamp/>
            </Simple_Form>
          </Simple_Form>
        </faims>
      </instance>
    </model>
  </h:head>
  <h:body>
    <!--START: Style-->
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <!--END: Style-->
    <group ref="User">
      <label>{User}</label>
      <group ref="User" faims_scrollable="false">
        <label>{User}</label>
        <select1 appearance="compact" ref="User_List">
          <label>{User_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Control">
      <label>{Control}</label>
      <group ref="Simple_Forms" faims_scrollable="false">
        <label>{Simple_Forms}</label>
        <trigger ref="New_Simple_Form">
          <label>{New_Simple_Form}</label>
        </trigger>
        <select1 appearance="compact" ref="Existing_Simple_Forms">
          <label>{Existing_Simple_Forms}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Map" faims_scrollable="false">
        <label>{Map}</label>
        <input faims_map="true" ref="Map">
          <label/>
        </input>
        <trigger ref="Center_Me_1">
          <label>{Center_Me}</label>
        </trigger>
      </group>
    </group>
    <group ref="Simple_Form" faims_archent_type="Simple Form">
      <label>{Simple_Form}</label>
      <group ref="Simple_Form">
        <label>{Simple_Form}</label>
        <input faims_attribute_name="ID" faims_attribute_type="measure" ref="ID">
          <label>{ID}</label>
        </input>
        <input faims_attribute_name="Name" faims_attribute_type="measure" ref="Name">
          <label>{Name}</label>
        </input>
        <group ref="Colgroup_1" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Latitude" faims_attribute_type="measure" ref="Latitude" faims_read_only="true">
              <label>{Latitude}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Longitude" faims_attribute_type="measure" ref="Longitude" faims_read_only="true">
              <label>{Longitude}</label>
            </input>
          </group>
        </group>
        <group ref="Colgroup_2" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Northing" faims_attribute_type="measure" ref="Northing" faims_read_only="true">
              <label>{Northing}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Easting" faims_attribute_type="measure" ref="Easting" faims_read_only="true">
              <label>{Easting}</label>
            </input>
          </group>
          <group ref="Col_3" faims_style="even">
            <label/>
            <input faims_attribute_name="Accuracy" faims_attribute_type="measure" ref="Accuracy" faims_read_only="true">
              <label>{Accuracy}</label>
            </input>
          </group>
        </group>
        <trigger ref="Take_From_GPS_1">
          <label>{Take_From_GPS}</label>
        </trigger>
        <select1 faims_attribute_name="Dropdown" faims_attribute_type="vocab" ref="Dropdown">
          <label>{Dropdown}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Simple Form timestamp" faims_attribute_type="measure" ref="Simple_Form_timestamp" faims_read_only="true">
          <label>{Timestamp}</label>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
